datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Donation {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String
  amount    Int
  createdAt DateTime @default(now())
}
